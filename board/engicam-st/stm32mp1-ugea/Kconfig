if TARGET_UGEA_STM32MP1

config SYS_BOARD
	default "stm32mp1-ugea"

config SYS_VENDOR
	default "engicam-st"

config SYS_CONFIG_NAME
	default "stm32mp1-ugea"

config CMD_STBOARD
	bool "stboard - command for OTP board information"
	default y
	help
	  This compile the stboard command to
	  read and write the board in the OTP.

endif
 
